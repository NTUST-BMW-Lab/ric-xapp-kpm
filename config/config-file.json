{

    "name": "kpm",
    "version": "0.0.1",
    "containers": [
        {
            "name": "kpm",
            "image": {
                "registry": "docker.io",
                "name": "mooncake22/kpm",
                "tag": "0.0.1"
            }
        }
    ],
    "livenessProbe": {
        "httpGet": {
            "path": "ric/v1/health/alive",
            "port": 8080
        },
        "initialDelaySeconds": 5,
        "periodSeconds": 15
    },
    "readinessProbe": {
        "httpGet": {
            "path": "ric/v1/health/ready",
            "port": 8080
        },
        "initialDelaySeconds": 5,
        "periodSeconds": 15
    },
    "messaging": {
        "ports": [
            {
                "name": "http",
                "container": "kpm",
                "port": 8080,
                "description": "http service"
            },
            {
                "name": "rmrroute",
                "container": "kpm",
                "port": 4561,
                "description": "rmr route port for kpm xapp"
            },
            {
                "name": "rmrdata",
                "container": "kpm",
                "port": 4560,
                "rxMessages": [
                    "RIC_INDICATION", 
                    "RIC_HEALTH_CHECK_REQ"
                ],
                "txMessages": [ 
                    "RIC_HEALTH_CHECK_RESP"
                ],
                "policies": [20002],
                "description": "rmr data port for kpm"
            }
        ]
    },
    "controls":{
        "logger": {
            "level": 4
        },
        "ASN-Print": true
    },
    "rmr":{
        "protPort": "tcp:4560",
        "maxSize": 2072,
        "numWorkers": 1,
        "rxMessages": [
            "RIC_INDICATION", 
            "RIC_HEALTH_CHECK_REQ"
        ],
        "txMessages": [ 
            "RIC_HEALTH_CHECK_RESP"
        ],
        "policies": [20002]
    },
    "db" : {
        "waitForSdl": false
    }

}